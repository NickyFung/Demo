//csslint规则的描述文件,
//详细了解可以去百度搜索“csslint 配置”关键字
//翻译版本地址：http://www.cnblogs.com/oltra/p/5138878.html
//官方API文档地址：https://github.com/CSSLint/csslint/wiki/Disallow-box-sizing
{
  //即使设置了box-sizing，仍然会warn，因为ie67不支持此属性
  "box-sizing":false,
  "box-model":false,
  "compatible-vendor-prefixes":false,
  "floats":false,
  "font-sizes":false,
  "gradients":false,
  "important":false,
  "known-properties": false,
  "outline-none":false,
  "qualified-headings": true,
  "regex-selectors":false,
  "shorthand":false,
  "text-indent":false,
  "unique-headings": false,
  "universal-selector": false,
  "unqualified-attributes": false
}